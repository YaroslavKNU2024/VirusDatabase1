@model VirusDatabaseNew.Symptom

@{
    ViewData["Title"] = "Видалити";
}

<h1>Видалити симптом</h1>

<h3>Ви впевнені, що хочете видалити симптом @Html.DisplayFor(modelItem => modelItem.SymptomName)?</h3>
<div>
    <h4>Симптоми</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SymptomName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SymptomName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Так" class="btn btn-danger" /> |
        <a asp-action="Index">Назад до списку</a>
    </form>
</div>
